Cell[BoxData[{
 RowBox[{
  RowBox[{"metric", ":=", 
   RowBox[{"metric", "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"-", 
